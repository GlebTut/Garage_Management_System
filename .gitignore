node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
package-lock.json

# PHP specific
*.php~
.phpunit.result.cache

# Database dumps and backups
*.sql
*.sql.gz
*.sql.zip
database_backup/
db_backups/

# Documentation build files (if using tools like phpDocumentor)
docs/build/
docs/api/

# Testing and coverage files
coverage/
.coverage
phpunit.xml
.phpunit.cache/

